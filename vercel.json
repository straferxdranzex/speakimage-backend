{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "installCommand": "bash install_rust_and_dependencies.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "PIP_NO_BINARY": ":maturin:"
    }
  }
}

{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
{
  "functions": {
    "app/api/**/*": {
      "maxDuration": 60 // All functions can run for a maximum of 5 seconds
    }
  }
}
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}
